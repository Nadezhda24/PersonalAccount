<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.personalaccount">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PersonalAccount">
        <activity android:name=".view.ChatActivity"></activity>
        <activity android:name=".view.EduSubjectActivity" />
        <activity android:name=".view.NoteActivity" />
        <activity android:name=".view.NoteListActivity" />
        <activity android:name=".view.EduActivity" />
        <activity android:name=".view.MessageSystemActivity" />
        <activity android:name=".view.ResultsActivity" />
        <activity android:name=".view.ScheduleActivity" />
        <activity android:name=".view.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>